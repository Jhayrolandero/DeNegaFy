---
import H4 from "../H4.astro";
import type { CollectionEntry } from "astro:content";
import { Image } from "astro:assets";

type Props = {
  contribute: CollectionEntry<"contributions">;
};

const { contribute } = Astro.props;
const { Content } = await contribute.render();
---

<article class="flex max-lg:flex-col max-lg:items-center">
  <div
    class="prose prose-xl prose-p:text-zinc-300 prose-headings:text-white prose-p:text-2xl"
  >
    <H4 title={contribute.data.title} />
    <Content />
  </div>
</article>
